    "typescript": "4.1.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "bootstrap": "4.5.3",
    "react": "17.0.1",
